package viveksingh.restful.bean;

/**
 * @author vivek_xz2hujv
 *
 */
public class Person {

	private String name;

	/**
	 * @param fName
	 * @param lName
	 */
	public Person(String name) {
		super();
		this.name = name;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

}
